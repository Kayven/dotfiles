<frame_geometry name="geometry_normal" rounded_top_left="true" rounded_top_right="true" rounded_bottom_left="true" rounded_bottom_right="true">
	<distance name="left_width" value="4"/>
	<distance name="right_width" value="4"/>
	<distance name="bottom_height" value="4"/>
	<distance name="left_titlebar_edge" value="6"/>
	<distance name="right_titlebar_edge" value="6"/>
	<distance name="button_width" value="22"/>
	<distance name="button_height" value="16"/>
	<distance name="title_vertical_pad" value="0"/>
	<border name="title_border" left="0" right="11" top="4" bottom="4"/>
	<border name="button_border" left="0" right="0" top="6" bottom="5"/>
</frame_geometry>

<frame_geometry name="geometry_maximized" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false" rounded_bottom_right="false">
	<distance name="left_width" value="1"/>
	<distance name="right_width" value="1"/>
	<distance name="bottom_height" value="1"/>
	<distance name="left_titlebar_edge" value="5"/>
	<distance name="right_titlebar_edge" value="5"/>
	<distance name="button_width" value="22"/>
	<distance name="button_height" value="16"/>
	<distance name="title_vertical_pad" value="0"/>
	<border name="title_border" left="0" right="11" top="2" bottom="2"/>
	<border name="button_border" left="0" right="0" top="6" bottom="5"/>
</frame_geometry>

<frame_geometry name="geometry_border" has_title="false">
	<distance name="left_width" value="4"/>
	<distance name="right_width" value="4"/>
	<distance name="bottom_height" value="4"/>
	<distance name="left_titlebar_edge" value="0"/>
	<distance name="right_titlebar_edge" value="0"/>
	<distance name="button_width" value="0"/>
	<distance name="button_height" value="0"/>
	<distance name="title_vertical_pad" value="0"/>
	<border name="title_border" left="0" right="0" top="4" bottom="0"/>
   	<border name="button_border" left="0" right="0" top="0" bottom="0"/>
</frame_geometry>


<!-- TITLEBARS -->
<draw_ops name="background_focused_normal">
	<!-- window border -->
	<rectangle color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" filled="false" x="0" y="0" width="width-1" height="height-1"/>
	
	<!-- background fill to fix theme manager rendering bugs -->
   	<rectangle color="shade/gtk:bg[NORMAL]/0.75" filled="true" x="0" y="0" width="width" height="top_height"/>

   	<!-- titlebar background -->	
	<gradient type="vertical" x="0" y="0" width="width" height="top_height">
		<color value="shade/gtk:bg[NORMAL]/0.20"/>
		<color value="shade/gtk:bg[NORMAL]/0.40"/>
	</gradient>
	<gradient type="vertical" x="0" y="0" width="width" height="top_height">
		<color value="shade/gtk:bg[NORMAL]/0.40"/>
		<color value="shade/gtk:bg[NORMAL]/0.20"/>
	</gradient>

	<!-- titlebar borders -->
	<!-- top -->
	<line color="shade/gtk:bg[NORMAL]/0.50" x1="5" y1="1" x2="width-6" y2="1"/>
	<!-- bottom -->
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="0" y1="top_height-1" x2="width" y2="top_height-1"/>
	
	<!-- corners -->
	<!-- top left -->
	<!-- outer highlights -->
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="3" y1="1" x2="4" y2="1"/>
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="2" y1="2" x2="2" y2="2"/>
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="1" y1="3" x2="1" y2="4"/>
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="0" y1="5" x2="0" y2="top_height-2"/>
	<!-- inner highlights -->
	<line color="shade/gtk:bg[NORMAL]/0.50" x1="3" y1="2" x2="4" y2="2"/>
	<line color="shade/gtk:bg[NORMAL]/0.50" x1="2" y1="3" x2="2" y2="4"/>
	<gradient type="vertical" x="1" y="5" width="1" height="7">
		<color value="shade/gtk:bg[NORMAL]/0.50"/>
		<color value="shade/gtk:bg[NORMAL]/0.50"/>
	</gradient>
	<!-- top right -->
	<!-- outer highlights -->
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="width-4" y1="1" x2="width-5" y2="1"/>
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="width-3" y1="2" x2="width-3" y2="2"/>
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="width-2" y1="3" x2="width-2" y2="4"/>
	<line color="shade/gtk:bg[NORMAL]/0.45" x1="width-1" y1="5" x2="width-1" y2="top_height-2"/>
	<!-- inner highlights -->
	<line color="shade/gtk:bg[NORMAL]/0.50" x1="width-4" y1="2" x2="width-5" y2="2"/>
	<line color="shade/gtk:bg[NORMAL]/0.50" x1="width-3" y1="3" x2="width-3" y2="4"/>
	<gradient type="vertical" x="width-2" y="5" width="1" height="7">
		<color value="shade/gtk:bg[NORMAL]/0.50"/>
		<color value="shade/gtk:bg[NORMAL]/0.50"/>
	</gradient>
	
		
	<!-- bottom left -->
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="0" y1="height-5" x2="0" y2="height-5"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.9" x1="1" y1="height-5" x2="1" y2="height-5"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="1" y1="height-4" x2="1" y2="height-4"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="2" y1="height-3" x2="2" y2="height-3"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="3" y1="height-2" x2="3" y2="height-2"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.9" x1="4" y1="height-2" x2="4" y2="height-2"/>
	<!-- bottom right -->
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="width-1" y1="height-5" x2="width-1" y2="height-5"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.9" x1="width-2" y1="height-5" x2="width-2" y2="height-5"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="width-2" y1="height-4" x2="width-2" y2="height-4"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="width-3" y1="height-3" x2="width-3" y2="height-3"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" x1="width-4" y1="height-2" x2="width-4" y2="height-2"/>
	<line color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.9" x1="width-5" y1="height-2" x2="width-5" y2="height-2"/>
	</draw_ops>

<draw_ops name="background_unfocused_normal">
