</draw_ops>

<draw_ops name="background_unfocused_maximized">
	<rectangle color="blend/gtk:fg[NORMAL]/gtk:bg[NORMAL]/0.8" filled="false" x="0" y="0" width="width-1" height="height-1"/>
</draw_ops>


<!-- TITLE -->
<draw_ops name="title_focused">
	<title color="#e5e5e5" x="(3 `max` (width-title_width + 15)) / 2" y="1 `max` ((height-title_height) / 2) + 1"/> 
</draw_ops>

