<!-- TITLEBARS -->
<draw_ops name="background_focused_normal">
	<!-- background fill to fix theme manager rendering bugs -->
   	<rectangle color="#444444" filled="true" x="0" y="0" width="width" height="height"/>

   	<!-- titlebar background -->
	<gradient type="vertical" x="0" y="0" width="width" height="top_height">
        	<color value="#4B4B4B"/>
		<color value="#444444"/>
	</gradient>

	<!-- titlebar borders -->
	<!-- top -->
	<line color="#000000" x1="4" y1="0" x2="width-5" y2="0"/>
	<!-- bottom -->
	<line color="#444444" x1="0" y1="top_height-1" x2="width" y2="top_height-1"/>

	<!-- gtk borders -->
	<line color="#000000" x1="0" y1="top_height" x2="0" y2="height"/>
	<line color="#000000" x1="0" y1="height-1" x2="width" y2="height-1"/>
	<line color="#000000" x1="width-1" y1="top_height" x2="width-1" y2="height-1"/>

	<!-- corners -->
	<!-- top left -->
	<!-- outer highlights -->
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="5" y1="0" x2="5" y2="0"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="3" y1="1" x2="4" y2="1"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="2" y1="2" x2="2" y2="2"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="1" y1="3" x2="1" y2="4"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="0" y1="5" x2="0" y2="top_height"/>
	<!-- top right -->
	<!-- outer highlights -->
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="width" y1="1" x2="width-1" y2="1"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="width-4" y1="1" x2="width-5" y2="1"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="width-3" y1="2" x2="width-3" y2="2"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="width-2" y1="3" x2="width-2" y2="4"/>
	<line color="shade/gtk:bg[SELECTED]/0.00" x1="width-1" y1="5" x2="width-1" y2="top_height"/>
	<line color="#000000" x1="0" y1="height" x2="0" y2="height"/>
	
</draw_ops>

